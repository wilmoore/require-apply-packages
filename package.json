  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "dependencies": {
    "fs-finder": "^1.8.0"
  },
  "devDependencies": {
    "commitizen": "^1.0.5",
    "cz-conventional-changelog": "^1.1.2",
    "koa": "^0.12.2",
    "mocha": "^2.3.3",
    "semantic-release": "^4.3.5",
    "standard": "^5.3.1",
    "supertest": "^0.14.0"
  },
  "homepage": "https://github.com/wilmoore/require-apply-packages",
